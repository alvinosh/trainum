{
  "name": "trainum",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "test:api": "dotenv -e ./apps/api/.env.test nx test api",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migrate:dev": "dotenv -e ./apps/api/.env npx prisma migrate dev",
    "migrate:test": "dotenv -e ./apps/api/.env.test npx prisma migrate dev",
    "clean:dev": "dotenv -e ./apps/api/.env npx prisma migrate reset",
    "clean:test": "dotenv -e ./apps/api/.env.test npx prisma migrate reset",
    "api": "nx build api --watch",
    "trainum": "nx serve trainum"
  },
  "prisma": {
    "schema": "apps/api/prisma/schema.prisma"
  },
  "private": true,
  "dependencies": {
